// Example for a basic file containing an entry return
#include <stdio.h>  // -*- lang: C -*-

int global_var = 2;

status AdditionalFunction(int x)
{
    // Printing a number
    printf("%i", x);

    // Creates and returns a status
    status r = { .status_code = 0 };
    return r;
}

entry status Main()
{
    int some_number = 4;

    some_number = some_number ? 4 : 5; // => 4
    some_number ? printf("yes") : printf("no");

    return AdditionalFunction(some_number);
}